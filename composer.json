{
    "name": "awcodes/filament-sentry",
    "type": "package",
    "description": "A basic auth scaffolding for Filament utilizing Breezy, Shield and custom User Resources.",
    "keywords": [
        "framework",
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/awcodes"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "bezhansalleh/filament-shield": "^2.0",
        "filament/filament": "^2.0",
        "jeffgreco13/filament-breezy": "^1.3",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "FilamentSentry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentSentry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentSentry\\FilamentSentryServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}